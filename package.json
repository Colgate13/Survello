{
  "name": "Survello",
  "version": "1.0.0",
  "description": "Template to create app with ts and DDD and Either",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "prisma:dev": "npx prisma migrate dev",
    "prisma:reset": "npx prisma migrate reset",
    "prisma:createOnly": "npx prisma migrate dev --create-only",
    "prisma:deploy": "npx prisma migrate deploy",
    "prisma:seed": "npx prisma db seed",
    "build": "tsc",
    "dev:web": "DEBUG=app:* ts-node --transpile-only src/app.ts start web",
    "start": "node dist/app.js start web --workers"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Colgate13/Survello.git"
